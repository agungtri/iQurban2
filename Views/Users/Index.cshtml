@model IEnumerable<iQurban.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PASSWORD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FIRST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LAST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EMAILID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PHONE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACCESS_LEVEL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.READ_ONLY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WRITE_ACCESS)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PASSWORD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FIRST_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LAST_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMAILID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PHONE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACCESS_LEVEL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.READ_ONLY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WRITE_ACCESS)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.USERID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.USERID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.USERID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
